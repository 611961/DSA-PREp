#include <iostream>
#include <queue>
using namespace std;

int main() {
  
    priority_queue<int> pq;

  
    pq.push(30);
    pq.push(10);
    pq.push(50);
    pq.push(20);

    cout << "Top element of the priority queue: " << pq.top() << endl;

  
    pq.pop();


    cout << "Top element after removing the top: " << pq.top() << endl;

    // Displaying and removing all elements from the priority queue
    cout << "Elements in the priority queue: ";
    while (!pq.empty()) {
        cout << pq.top() << " ";
        pq.pop();
    }

    return 0;
}
